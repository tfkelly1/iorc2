


<%= semantic_form_for @record do |f| %>
  <%= f.inputs do %>
	<%= f.input :status, :include_blank => false %>
	<%= f.input :bird, :include_blank => false, :member_label => :common %>
    <%= f.input :quantity %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>


